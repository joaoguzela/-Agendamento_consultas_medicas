{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^node:(.*)",
    "<THIRD_PARTY_MODULES>",
    "^#(.*)/",
    "^[./]"
  ],
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "endOfLine": "lf",
  "trailingComma": "all",
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy",
    "importAssertions"
  ]
}